package ex05_string;

public class Ex01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		// 문자열 연결 연산자 
		// 문자열이 포함된 + 연산은 연결
		
		String str1 = "구디" + "아카데미" ; 
		String str2 = 100 + "번지" ; 
		String sir3 = 1+1+"행사";
		
		System.out.println(str1);
		System.out.println(str2);
		System.out.println(str3);
		
		
		
		
		
		
		
		
	}

}
